-void
-linSwap(FxU32 code)
-{
-  if (IsFullScreen) return;
-  XCopyArea(dpy, pixBack, hWndApp, xgc, 0, 0, Width, Height, 0, 0);
-  XSync(dpy, False);
-}  /* linSwap */
