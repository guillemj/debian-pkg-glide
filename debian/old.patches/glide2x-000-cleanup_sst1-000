-  CRTC_SET(0x70, tmp);
-
-  /* Reset the current display buffer bits (0-1) */
-  {
-    FxU8 crtcSwapBuffer;
-
-    OUTP(0x3D4, 0x7A);
-    INP(0x3D5, crtcSwapBuffer);
-
-    crtcSwapBuffer &= ~0x03;
-    CRTC_SET(0x7A, crtcSwapBuffer);
-  }
