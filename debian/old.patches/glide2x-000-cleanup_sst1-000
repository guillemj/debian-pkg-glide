-static FxBool
-cvtAddrToBufDesc(void *pixAddr, InitBufDesc_t *pDesc) {
-  pDesc->bufOffset = (FxU32)pixAddr;
-  pDesc->bufStride = (FxI32)(2*screenWidth);
-  pDesc->bufBPP    = (FxI32)16;
-  return FXTRUE;
-}
