-#define GETREGVALFROMENV(val, envStr, constant)\
-{\
-  const char *envVal;\
-  val = constant;\
-  if ((envVal = myGetenv(envStr)) != NULL) {\
-    sscanf(envVal, "%x", &val);\
-    GDBG_INFO((80,\
-               "%s:  The environment variable %s == 0x%x overloaded a register value\n", \
-               FN_NAME, envStr, val));\
-  }\
